<?xml version='1.0' encoding='utf-8'?>
<widget id="com.swoopit.android" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>SwoopIt</name>
    <description>
        Food Delivery For Schools.
    </description>
    <author email="mdean@hpa.edu" href="https://swoopit.xyz">
        Morgan Dean
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <resource-file src="res/swoopit.png" target="res/swoopit.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <icon density="mdpi" src="res/icon/android/mdpi.png" />
    <icon density="hdpi" src="res/icon/android/hdpi.png" />
    <icon density="xhdpi" src="res/icon/android/xhdpi.png" />
    <icon density="xxhdpi" src="res/icon/android/xxhdpi.png" />
    <icon density="xxxhdpi" src="res/icon/android/xxxhdpi.png" />
    <preference name="StatusBarBackgroundColor" value="#1cafef" />
    <preference name="orientation" value="portrait" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarStyle" value="blacktranslucent" />
    <preference name="StatusBarBackgroundColor" value="#1cafef" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="phonegap-plugin-push" spec="^2.2.2">
        <variable name="FCM_VERSION" value="11.8.0" />
        <variable name="ANDROID_SUPPORT_V13_VERSION" value="27.+" />
    </plugin>
    <plugin name="cordova-plugin-statusbar" spec="^2.4.2" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="UIWebViewBounce" value="false" />
    <plugin name="cordova-plugin-googleplus" spec="^5.2.3">
        <variable name="REVERSED_CLIENT_ID" value="com.googleusercontent.apps.396697495271-8j9q2va5brpq7ot55n23bs15nsgmvfbf" />
    </plugin>
    <plugin name="cordova-plugin-keyboard" spec="^1.2.0" />
</widget>
